	}
	public void setvalues(String val) {
		values = val;
	}
	public String getcampaignIdLabels() {
		return campaignIdLabels;
	}
	public void setcampaignIdLabels(String val) {
		campaignIdLabels = val;
	}
	public String getreportStatus() {
